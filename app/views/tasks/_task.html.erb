<div id="<%= dom_id task %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= task.name %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= task.description %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Expiration date:</strong>
    <%= task.expiration_date %>
  </p>
</div>
